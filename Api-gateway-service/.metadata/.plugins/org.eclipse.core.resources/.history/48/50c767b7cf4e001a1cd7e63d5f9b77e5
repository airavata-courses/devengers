package com.devengers.service.sessionmanagement.model;

import java.util.Date;

import javax.persistence.*;


@Entity
public class UserSessionDetail {

		@Id
		@Column(name="userId")
		private Integer userid;
		
		@Column(name="name")
	    private Integer correlationid;
		
		@Column(name="no_of_files ")
	    private Integer no_of_files;
		
		@Temporal(TemporalType.TIMESTAMP)
		@Column(name = "edit_timestamp", 
		        updatable = false,
		        columnDefinition="TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP")
		private Date editTimestamp;
		
	    public Integer getUserid() {
			return userid;
		}

		public void setUserid(Integer userid) {
			this.userid = userid;
		}

	    public int getCorrelationId() {
	        return correlationid;
	    }

	    public void setCorrelationId(int correlationid) {
	        this.correlationid = correlationid;
	    }

	    public int getNo_of_files() {
			return no_of_files;
		}

		public void setNo_of_files(int no_of_files) {
			this.no_of_files = no_of_files;
		}

}
