package com.devengers.service.sessionmanagement.repository2;

import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.devengers.service.sessionmangement.model.UserSessionDetail;

import java.util.List;

@Repository
public interface SessionDetailsRepository extends CrudRepository<UserSessionDetail, Integer> {
    List<UserSessionDetail> findByUserName(int i);
}
