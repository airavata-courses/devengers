package com.devengers.sessionhandling.sessionservice.model;

import java.util.List;

import com.fasterxml.jackson.annotation.JsonIdentityInfo;
import com.fasterxml.jackson.annotation.ObjectIdGenerators;

@JsonIdentityInfo(generator = ObjectIdGenerators.IntSequenceGenerator.class, property = "@id", scope = UserSessionDetails.class)
public class UserSessionDetails {

    private int userId;
    private int correlationId;

    public UserSessionDetails() {
    }

    public UserSessionDetails(int userName, int quotes) {
        this.userId = userName;
        this.correlationId = quotes;
    }

    public int getUserName() {
        return userId;
    }

    public void setUserName(int userName) {
        this.userId = userName;
    }

    public int getQuotes() {
        return correlationId;
    }

    public void setQuotes(int quotes) {
        this.correlationId = quotes;
    }
}
